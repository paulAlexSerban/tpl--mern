{
    "name": "@wbk--mern-playground/rnd-react-query-n-tanstack-todo-list-api",
    "version": "0.0.0",
    "private": true,
    "main": "dist/bin/www.js",
    "scripts": {
        "develop": "concurrently \"yarn tsc --watch\" \"nodemon -q /dist/bin/www.js\"",
        "start": "pm2 start ./dist/bin/www.js -i 0",
        "stop": "pm2 delete www",
        "monitor": "pm2 monit",
        "build:dev": "yarn tsc",
        "build:prod": "yarn tsc",
        "docker:build:local": "docker build -t 'wbk--mern-playground/rnd-react-query-n-tanstack-todo-list-api' --file dev.Dockerfile .",
        "docker:build:prod": "docker build -t 'wbk--mern-playground/rnd-react-query-n-tanstack-todo-list-api' --file prod.Dockerfile .",
        "docker:run:local": "docker run -p 5000:5000 'wbk--mern-playground/rnd-react-query-n-tanstack-todo-list-api'",
        "docker:run:prod": "docker run -p 5000:5000 'wbk--mern-playground/rnd-react-query-n-tanstack-todo-list-api'"
    },
    "dependencies": {
        "@types/cookie-parser": "^1.4.6",
        "@types/debug": "^4.1.12",
        "@types/express": "^4.17.21",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.0",
        "body-parser": "^1.20.2",
        "concurrently": "^8.2.2",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "^4.18.2",
        "morgan": "~1.9.1",
        "nodemon": "^2.0.22",
        "pm2": "^5.3.1",
        "typescript": "^5.2.2"
    }
}
