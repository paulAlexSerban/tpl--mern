        "@types/node-geocoder": "^4.2.6",
        "concurrently": "^8.2.2",
        "nodemon": "^2.0.22",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "axios": "^1.7.4",
        "debug": "~2.6.9",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "morgan": "~1.9.1"
    }
}
