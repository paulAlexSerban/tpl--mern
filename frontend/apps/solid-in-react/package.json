{
    "name": "@wbk--mern-playground/solid-in-react",
    "formattedName": "Vite React TypeScript Boilerplate",
    "description": "A Vite-based React TypeScript boilerplate with ESLint, Prettier, and Sass support.",
    "private": true,
    "version": "0.2.0",
    "type": "module",
    "scripts": {
        "develop": "vite --host",
        "build:dev": "export NODE_ENV=development && tsc && vite build",
        "build:prod": "export NODE_ENV=production && tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "package": "bash scripts/package.bash"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.16.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/react": "^18.2.27",
        "@types/react-dom": "^18.2.12",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "axios": "^1.6.2",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "sass": "^1.69.5",
        "typescript": "^5.2.2",
        "vite": "^4.4.5"
    }
}
